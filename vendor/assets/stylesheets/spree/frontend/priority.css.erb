/* ====================================================================================================================
 * FONT-FACE
 * ====================================================================================================================*/
@font-face {
  font-family: 'flexslider-icon';
  src: url(<%= asset_path 'spree/frontend/fonts/flexslider-icon.eot' %>);
  src: url(<%= asset_path 'spree/frontend/fonts/flexslider-icon.eot?#iefix' %>) format('embedded-opentype'), url(<%= asset_path 'spree/frontend/fonts/flexslider-icon.woff' %>) format('woff'), url(<%= asset_path 'spree/frontend/fonts/flexslider-icon.ttf' %>) format('truetype'), url(<%= asset_path 'spree/frontend/fonts/flexslider-icon.svg#flexslider-icon' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}
/* priority css */
nav #main-nav-bar li a,nav#top-nav-bar ul li a,nav #main-nav-bar li a:hover,nav#top-nav-bar ul li a:hover
{
	color:#fff !important;
	text-transform:uppercase !important;
	font-weight:normal !important;
	font-size:15px !important;
}
