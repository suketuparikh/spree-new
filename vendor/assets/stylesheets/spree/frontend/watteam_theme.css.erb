#footer .container .f_mid{padding-bottom: 22px !important;width: 45% !important;}
#footer .container .f_right{padding-bottom: 22px !important;width: 100% !important;text-align: center;}
#homepage-logo-spree{padding-top: 70px;}
#homepage-logo-spree {background: rgba(0, 0, 0, 0) url("/assets/powerbeat_logo-5878413cb01410394236b8adcb1ad70f.png") no-repeat scroll 0 0 / 58% 52% !important;}
#wrapper.container{width:100%!important;}

}





@media only screen and  (max-width:660px){
.product-image > a {    margin: 0 auto;}
body ul#products li.product-row {    width: 100%;}
body ul#products [data-hook="products_list_item"]{width:100%!important;}
}


@media only screen and  (max-width:610px){
.togle_menu_mobile{display:block;}
#menu-mainmenu{display:none;}
.togle_menu_mobile {    background-color: #03737E;    display: block;}
.pro_arro.menu_mo > a {color:#fff!important;    display: block;    float: left;    text-align: left;   width: 100%;}
.pro_arro.menu_mo > a:hover{color:#135785 !important;} 
.submenu {    color: #fff;    padding-left: 20px;}
.main-menu-top ul{float:none;}
.mobile-product {width:100%; float:left;}
.menu_mo a ,.menu_mo ul li a  {color:#fff !important; }
.togle_menu_mobile{padding:5px; margin-top:2px;}
#mobile-menu-open
{
color:#fff !important;
display:block;
font-size: 16px;
margin-top: 2px;
padding:5px; 
}

}

@media only screen and  (max-width:600px){
.popup-main {display:none !important;}
#homepage-caption{ padding: 2px 4px;    width: 300px;}
.powerbeat-sub-subtitle{font-size: 12px;}
#menu-mainmenu li {    display: block;    float: left;    padding: 8px 14px;}

}



@media only screen and  (max-width:480px){
#wrapper.container{ width:300px !important;}
.left-benefits, .right-benefits{ text-align: left; width: 85%;}
.benefits-main p{ line-height:18px;}
body ol.progress-steps li span,ol.progress-steps li{ font-size:13px !important; display:inline !important;}
body footer#footer h5{font-family:lato; border-color:#cecece !important;}
body table#cart-detail th, body table#cart-detail td, body table#cart-detail tbody tr td[data-hook="cart_item_price"], body table#cart-detail tbody#line_items tr td[data-hook="cart_item_total"]{ padding:5px;}
body #wrapper #clear_cart_link a, body #wrapper #clear_cart_link button, body #wrapper #clear_cart_link input, body #wrapper #update-cart .links a, body #wrapper #update-cart .links button, body #wrapper #update-cart .links input{ padding: 3px 2px !important;}
#footer .container .f_right{padding-bottom: 22px !important;width: 100% !important;text-align: center;}
#footer .container .f_mid{padding-bottom: 22px !important;width: 100% !important;text-align: center;}
#footer .container .f_left{padding-bottom: 22px !important;width: 100% !important;text-align: center;}
.flexslider .slides img {    height: 245px;}
#homepage-caption{ padding: 7px 1px;}
.powerbeat-sub-subtitle{font-size: 12px;}
}




@media only screen and  (max-width:360px){
body table#cart-detail th, body table#cart-detail td, body table#cart-detail tbody tr td[data-hook="cart_item_price"], body table#cart-detail tbody#line_items tr td[data-hook="cart_item_total"] {
    padding: 0;
}

}


